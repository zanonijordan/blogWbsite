<%- include('./partials/header')%>
<h1>Home</h1>
<p>Welcome to my Blog!</p>
<% posts.forEach((post, i) => { %>
  <h1><%= posts[i].title %></h1>
  <p>
    <%= posts[i].content.substring(0, 100) +' ... ' %>
    <a href="/posts/+<%=posts[i].title%>">Read More</a>
  </p>
<% }) %>
